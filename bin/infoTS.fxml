<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProgramController">
   <children>
      <HBox prefHeight="100.0" prefWidth="400.0">
         <children>
            <TextArea fx:id="infoID" prefHeight="100.0" prefWidth="400.0" promptText="INFO" wrapText="true">
               <font>
                  <Font size="20.0" />
               </font>
            </TextArea>
            <VBox prefHeight="200.0" prefWidth="50.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#INFOfinishButton" prefHeight="50.0" prefWidth="50.0" text="F" />
                  <Button mnemonicParsing="false" onAction="#INFOcancelButton" prefHeight="50.0" prefWidth="50.0" text="C" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
