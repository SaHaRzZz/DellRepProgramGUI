<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="350.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="ProgramController">
   <children>
      <VBox prefHeight="350.0" prefWidth="400.0">
         <children>
            <TextArea fx:id="TStextID" prefHeight="400.0" prefWidth="400.0" />
            <HBox prefHeight="20.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#TSfinishButton" prefHeight="20.0" prefWidth="200.0" text="Finish" />
                  <Button mnemonicParsing="false" onAction="#TSshowButton" text="S" />
                  <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#TScancelButton" prefHeight="20.0" prefWidth="200.0" text="Cancel" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
